{
  "expo": {
    "name": "Birthday Reminder",
    "slug": "birthday-reminder",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mertjs.birthdayreminder"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f7a6f174-bbc3-423e-b702-59c2abee841b"
      },
      "expo": {
        "backgroundFetch": {
          "checkInterval": 3600,
          "minimumInterval": 86400,
          "forceAlarmManager": true
        }
      }
    },
    "owner": "mertjs"
  }
}
